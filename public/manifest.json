{
    "name": "EpicVPN Vue",
    "description": "EpicVPN Vue Chrome Extension",
    "author": "Tskhe Dmitry <tskhe95@gmail.com> and Stefan Markovic <zmstefan@gmail.com>",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "128": "images/icon128.png"
    },
    "background": {
        "service_worker": "worker.js"
    },
    "content_scripts": [{ "matches": ["<all_urls>"], "js": ["content-script.js"] }],
    "permissions": ["management", "storage", "privacy", "proxy", "browsingData", "webRequest", "webRequestAuthProvider", "cookies", "notifications", "declarativeNetRequest", "declarativeNetRequestFeedback"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_title": "EpicVPN",
        "default_popup": "index.html"
    }
}
